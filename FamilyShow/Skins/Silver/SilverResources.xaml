      <Border
                Margin="2"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                DockPanel.Dock="Top">
        <Image
                    MinWidth="65"
                    MinHeight="65"
                    MaxWidth="65"
                    MaxHeight="65"
                    Source="{Binding FullyQualifiedPath, Converter={StaticResource ImageConverter}}"
                    Stretch="UniformToFill" />
      </Border>
      <TextBlock
                Margin="0,-10,5,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                DockPanel.Dock="Bottom"
                FontSize="36"
                FontWeight="Bold"
                Foreground="#FF000000"
                Text="{Binding IsAvatar, Converter={StaticResource PrimaryAvatarConverter}}" />
    </Grid>
  </DataTemplate>

